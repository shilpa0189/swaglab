<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
  <test thread-count="5" name="FunctionalTest(Chrome)">
  <parameter name="browser" value="Chrome"></parameter>
    <classes>
      <class name="test.VerifyLoginFunctionality">
      <methods>
      <include name="verifyCustomerShouldBeAbledToAddMultipleProduct"></include>
      <include name="verifyIfCustomerAbleToPlaceAnOrder"></include>
      <include name="verifyCustomerShouldBeAbledToaddProductInTheCart"></include>
      </methods>
      </class>
    </classes>
  </test> <!-- Test -->
  <test thread-count="5" name="FunctionalTest(Edge)">
  <parameter name="browser" value="Edge"></parameter>
   <classes>
    <class name="test.VerifyLoginFunctionality">
    <methods>
      <include name="verifyCustomerShouldBeAbledToAddMultipleProduct"></include>
      <include name="verifyIfCustomerAbleToPlaceAnOrder"></include>
      <include name="verifyCustomerShouldBeAbledToaddProductInTheCart"></include>
      </methods>
      </class>
      </classes>
  </test>
</suite> <!-- Suite -->
